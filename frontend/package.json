        "url": "https://github.com/andreluisce/front-end-recruitment.git/issues"
    },
    "homepage": "https://github.com/andreluisce/front-end-recruitment.git#readme",
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^20.0.3",
        "babel-loader": "^7.0.0",
